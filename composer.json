{
    "name": "docker-php/docker-php",
    "license": "MIT",
    "type": "library",
    "description": "A Docker PHP client",
    "autoload": {
        "psr-4": {
            "Docker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Docker\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "jane-php/open-api-runtime": "^7.10",
        "guzzlehttp/psr7": "^2.8",
        "php-http/client-common": "^2.7",
        "php-http/socket-client": "^2.4",
        "php-http/message": "^1.0",
        "symfony/filesystem": "^7.4 || ^8.0",
        "symfony/process": "^7.4 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0",
        "sigwin/infra": "~1.24.0",
        "jane-php/open-api-2": "^7.10"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
