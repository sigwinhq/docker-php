{
    "name": "sigwin/docker-php",
    "description": "A Docker PHP client",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.3",
        "jane-php/open-api-runtime": "^7.10",
        "php-http/discovery": "^1.20",
        "symfony/filesystem": "^7.4 || ^8.0",
        "symfony/http-client": "^8.0",
        "symfony/process": "^7.4 || ^8.0"
    },
    "require-dev": {
        "jane-php/open-api-2": "^7.10",
        "phpunit/phpunit": "^12.0",
        "sigwin/infra": "~1.24.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Docker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Docker\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
